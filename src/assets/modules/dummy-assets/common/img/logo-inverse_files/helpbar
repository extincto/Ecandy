Helpbar=function(n){var t=this;t.dom;t.drawningboard=n;t.closex;t.helphandle;t.questions;t.questionswrapper;t.drawningboard.api.loadTemplate("helpbar",function(n){t.dom=$("#helpbar");t.dom.append($(n));t.helphandle=t.dom.find(".helphandle");t.closex=t.dom.find(".closex");t.questionswrapper=t.dom.find(".questionwrapper");t.questions=t.dom.find(".question");t.helphandle.on("click",function(){t.dom.toggleClass("open");t.questionswrapper.removeClass("open")});t.closex.on("click",function(){t.dom.toggleClass("open");t.questionswrapper.removeClass("open")});t.questions.on("click",function(){var r=$(this),i=!1,n=r.closest(".questionwrapper");n.hasClass("open")&&(i=!0);i?n.removeClass("open"):(t.questionswrapper.removeClass("open"),n.addClass("open"))})})}